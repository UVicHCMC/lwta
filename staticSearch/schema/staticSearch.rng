   <define name="tokenizer">
      <element name="tokenizer" ns="http://hcmc.uvic.ca/ns/staticSearch">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Configures options for the tokenizing process.) </a:documentation>
         <empty/>
         <optional>
