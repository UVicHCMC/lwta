   <define name="context">
      <element name="context" ns="http://hcmc.uvic.ca/ns/staticSearch">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(A context definition, providing a match attribute that identifies the context, allowing keyword-in-context fragments to be bounded by a specific context.) </a:documentation>
         <empty/>
         <sch:ns xmlns="http://www.tei-c.org/ns/1.0"
                 xmlns:math="http://www.w3.org/1998/Math/MathML"
                 xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:svg="http://www.w3.org/2000/svg"
                 xmlns:xh="http://www.w3.org/1999/xhtml"
                 xmlns:xi="http://www.w3.org/2001/XInclude"
                 uri="http://hcmc.uvic.ca/ns/staticSearch"
                 prefix="ss"/>
         <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:math="http://www.w3.org/1998/Math/MathML"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:svg="http://www.w3.org/2000/svg"
                      xmlns:xh="http://www.w3.org/1999/xhtml"
                      xmlns:xi="http://www.w3.org/2001/XInclude">
            <sch:rule context="ss:context">
               <sch:assert test="not(@label and @context = 'false')">
                      ERROR: If a context has a label, it must be a context for the purposes of indexing.
                    </sch:assert>
            </sch:rule>
         </sch:pattern>
         <ref name="att.match.attributes"/>
         <ref name="att.labelled.attributes"/>
         <optional>
