   <define name="exclude">
      <element name="exclude" ns="http://hcmc.uvic.ca/ns/staticSearch">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(An exclusion definition, which excludes either documents or filters as defined by an XPath in the match attribute.) </a:documentation>
         <empty/>
         <ref name="att.match.attributes"/>
