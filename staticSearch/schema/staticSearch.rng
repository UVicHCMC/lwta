   <define name="results">
      <element name="results" ns="http://hcmc.uvic.ca/ns/staticSearch">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Controls the configuration of the results page.) </a:documentation>
         <empty/>
